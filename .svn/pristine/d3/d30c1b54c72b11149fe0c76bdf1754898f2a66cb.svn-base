package com.isolver.dto.wechat;

import java.util.Date;

public class SalaryListDto {

	/** 工资条id */
	private Long id;
	/** 工资日期 */
	private Date date;
	/** version */
	private Integer version;

	/**
	 * @return the id
	 */
	public Long getId() {
		return id;
	}

	/**
	 * @param id the id to set
	 */
	public void setId(Long id) {
		this.id = id;
	}

	/**
	 * @return the date
	 */
	public Date getDate() {
		return date;
	}

	/**
	 * @param date the date to set
	 */
	public void setDate(Date date) {
		this.date = date;
	}

	/**
	 * @return the version
	 */
	public Integer getVersion() {
		return version;
	}

	/**
	 * @param version the version to set
	 */
	public void setVersion(Integer version) {
		this.version = version;
	}

	/**
	 * SalaryListDto.java
	 * 
	 * @param id
	 * @param date
	 * @param version
	 * 
	 */
	public SalaryListDto(Long id, Date date, Integer version) {
		super();
		this.id = id;
		this.date = date;
		this.version = version;
	}

}
